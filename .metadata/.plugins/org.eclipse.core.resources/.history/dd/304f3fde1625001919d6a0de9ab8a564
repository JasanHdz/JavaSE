package com.jasanhdz.amazonviewer.model;

public class Serie extends Film {
	
	private int id;
	private int sessionQuatity; 
	private Chapter[] chapter;
	
	public Serie(String title, String genre, String creator, int duration, int sessionQuatity) {
		super(title, genre, creator, duration);
		this.sessionQuatity = sessionQuatity;
	}
	
	public int getId() {
		return id;
	}

}
